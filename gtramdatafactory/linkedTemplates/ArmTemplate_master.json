{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gtramdatafactory"
		},
		"datalake_link_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_link'"
		},
		"datalake_link_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gtramstorageadmin.dfs.core.windows.net/"
		},
		"openweather_api_airpol_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org"
		},
		"openweather_api_link_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/"
		},
		"vault_link_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ds598-vault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gtramdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake_link_accountKey": {
						"value": "[parameters('datalake_link_accountKey')]"
					},
					"datalake_link_properties_typeProperties_url": {
						"value": "[parameters('datalake_link_properties_typeProperties_url')]"
					},
					"openweather_api_airpol_properties_typeProperties_url": {
						"value": "[parameters('openweather_api_airpol_properties_typeProperties_url')]"
					},
					"openweather_api_link_properties_typeProperties_url": {
						"value": "[parameters('openweather_api_link_properties_typeProperties_url')]"
					},
					"vault_link_properties_typeProperties_baseUrl": {
						"value": "[parameters('vault_link_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}