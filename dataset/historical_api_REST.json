{
	"name": "historical_api_REST",
	"properties": {
		"linkedServiceName": {
			"referenceName": "openweather_api_link",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lat": {
				"type": "string",
				"defaultValue": "42.3601"
			},
			"lon": {
				"type": "string",
				"defaultValue": "-71.0589"
			},
			"dataType": {
				"type": "string",
				"defaultValue": "hour"
			},
			"start": {
				"type": "string"
			},
			"end": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestResource",
		"typeProperties": {
			"relativeUrl": "data/2.5/history/city?lat=@{dataset().lat}&lon=@{dataset().lon}&type=@{dataset().dataType}&start=@{dataset().start}&end=@{dataset().end}&appid=@{dataset().appid}"
		},
		"schema": []
	}
}